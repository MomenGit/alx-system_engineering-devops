[Unit]
Description=Gunicorn instance to serve AirBnB clone - Web dynamic
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=gunicorn --workers 3 --bind 0.0.0.0:5003 -t 60 --access-logfile /tmp/airbnb-access.log --error-logfile /tmp/airbnb-error.log  web_dynamic.2-hbnb:app
Restart=always

[Install]
WantedBy=multi-user.target
